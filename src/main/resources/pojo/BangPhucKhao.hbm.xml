<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.BangPhucKhao" table="bangphuckhao" catalog="quanlysinhvien" optimistic-lock="version">
        <id name="id" type="string">
            <column name="idBang" length="10" />
            <generator class="assigned" />
        </id>
        <property name="ngayBatDau" type="string">
            <column name="ngayBatDau" length="20" />
        </property>
        <property name="ngayKetThuc" type="string">
            <column name="ngayKetThuc" length="20" />
        </property>
    </class>
</hibernate-mapping>